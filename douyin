<script>
    // 解决B站图床问题
    const isSafari = /Safari/.test(navigator.userAgent);
    const isIPhone = /iPhone/.test(navigator.userAgent);
    if (isIPhone && !isSafari) {
        document.querySelectorAll('img').forEach((img) => {
            const isBiliImg = ['hdslb.com', 'biliimg.com'].some((domain) => img.src.includes(domain));
            if (isBiliImg) {
                img.src = 'https://image.baidu.com/search/down?thumburl=https://baidu.com&url='+img.src;
            }
        });
    }
</script>

